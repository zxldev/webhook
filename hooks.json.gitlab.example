[
  {
    "id": "webhook",
    "execute-command": "./webHookRouter.sh",
    "command-working-directory": "{AB_PATH_TO_ROOT}",
    "pass-arguments-to-command": [
      {
        "source": "payload",
        "name": "project.name"
      },
      {
        "source": "payload",
        "name": "ref"
      },
      {
        "source": "payload",
        "name": "project.namespace"
      }
    ],
    "trigger-rule": {
      "and": [
        {
          "match": {
            "type": "value",
            "value": "{password}",
            "parameter": {
              "source": "header",
              "name": "X-Gitlab-Token"
            }
          }
        },
        {
          "match": {
            "type": "value",
            "value": "refs/heads/master",
            "parameter": {
              "source": "payload",
              "name": "ref"
            }
          }
        }
      ]
    }
  }
]
